[
        {
            "name": "nginx",
            "image": "nginx",
            "essential": true,
            "cpu": 10,
            "memory": 500,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "fargate-container-logs",
                    "awslogs-region": "eu-west-1",
                    "awslogs-stream-prefix": "awslogs-fargate"
                }
            }
        }
]